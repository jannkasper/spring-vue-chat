{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "noImplicitAny": false,
    "allowJs": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
